{
  "version": "1.0",
  "source": "mc_functions.json",
  "category": "mc_sync",
  "functions": [
    {
      "name": "MC_VelocitySynchronization",
      "axis_types": [
        "TO_SynchronousAxis"
      ],
      "inputs": [
        "Master",
        "Slave",
        "Ratio",
        "Tolerance",
        "Execute"
      ],
      "outputs": [
        "Done",
        "Busy",
        "Error",
        "ErrorID"
      ],
      "lint": [
        "Master and Slave cannot be same axis",
        "Ratio > 0",
        "Tolerance >= 0"
      ]
    },
    {
      "name": "MC_PositionSynchronization",
      "axis_types": [
        "TO_SynchronousAxis"
      ],
      "inputs": [
        "Master",
        "Slave",
        "StartPosition",
        "MasterStartPosition",
        "Execute"
      ],
      "outputs": [
        "Done",
        "Busy",
        "Error",
        "ErrorID"
      ],
      "lint": [
        "Master and Slave cannot be same axis"
      ]
    }
  ]
}
