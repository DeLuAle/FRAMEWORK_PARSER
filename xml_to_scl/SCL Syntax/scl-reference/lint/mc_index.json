  "errors": {
    "0x1001": {
      "description": "Axis disabled",
      "action": "MC_Power Enable TRUE"
    },
    "0x1002": {
      "description": "Homing required",
      "action": "MC_Home first"
    },
    "0x1004": {
      "description": "Axis in error state",
      "action": "MC_Reset Restart TRUE"
    },
    "0x2001": {
      "description": "Position underflow",
      "action": "Adjust position >= MinPosition"
    },
    "0x2002": {
      "description": "Position overflow",
      "action": "Adjust position <= MaxPosition"
    },
    "0x4004": {
      "description": "Acceleration invalid",
      "action": "Acceleration > 0"
    },
    "0x4005": {
      "description": "Deceleration invalid",
      "action": "Deceleration > 0"
    },
    "0x4006": {
      "description": "Velocity invalid",
      "action": "Velocity > 0 or allowed zero for MoveVelocity"
    }
