<?xml version="1.0" encoding="utf-8"?>
<Document>
  <Engineering version="V19" />
  <SW.Types.PlcStruct ID="0">
    <AttributeList>
      <Access>Published</Access>
      <Interface><Sections xmlns="http://www.siemens.com/automation/Openness/SW/Interface/v5">
  <Section Name="None">
    <Member Name="O_ID" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">ID PLC per riferimento allìordine di produzione</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Type" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">1= Caricato da MES; 2= Caricamento HMI manuale</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Status" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">0 = Non iniziato; 1 = Iniziato; 2 = In lavoro; 3 = Conclusione Parziale; 4 = Concluso completo; 5 = Concluso complerto; 6 = Pausa parziale; 7 = Pausa totale</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Status_Saved" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">Stato salvato = Su variazione rispetto a "Status" sono inviati messaggi a MES</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Material_Len_REQ" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Quanto materiale è richiesto per eseguire l'ordine</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Material_Len_ToGo" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Quanto materiale manca per concludere l'ordine</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="MES_IDP" Datatype="String[30]">
      <Comment>
        <MultiLanguageText Lang="en-US">ID MES per riferimento all'ordine di prosuzione</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="MES_Profile_Code" Datatype="String[3]">
      <Comment>
        <MultiLanguageText Lang="en-US">Codice profilo dei pezzi da produrre: profilo accoppiato a C (sigla codice S); profilo semiguscio SG (sigla codice G)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="MES_PPrf_Name" Datatype="String[20]">
      <Comment>
        <MultiLanguageText Lang="en-US">Nome dell programma profilo richiesto da MES (es.C040x45x1.25)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="PPrf_No" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Numero del programma profillo che corrisponde al codice MES del prodotto da eseguire</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beam_Code" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">Codice Pezzo: 0= SG formato da una singola parte; 1= C/S = Formato da due parti accoppiate</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_W_A" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Dim. base pezzo (W) = Altezza in impilamento (A)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_H_B" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Dim. altezza pezzo (H) = base in impilamento (B)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_Thickness" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Spessore lamiera del profilo</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_Len_Min" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Lunghezza pezzo - Tolleranza minima</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_Len_Max" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Lunghezza pezzo + Tolleranza massima</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_Len" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Lunghezza pezzi richiesta</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_REQ" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Quantità di pezzi da produrre</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_ToGo" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Quante pezzi mancano per concludere l'ordine</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_NonConforming" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Somma Beams_NG + Beams_TOCHECK</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_GOOD" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Pezzi prodotti con qualità CONFORMI  (2 parti = 1 pezzo)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_GOOD_R" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Pezzi prodotti con qualità CONFORMI  dopo il RICONIDZIONAMENTO</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_NG" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Pezzi prodotti con qualità NG = Scartati dopo controllo qualità</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Beams_TOCHECK" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Pezzi prodotti che attendono controllo qualità</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_Len" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Lunghezza delle parti con target al centro della tolleranza richiesta</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_Tol" Datatype="Real">
      <Comment>
        <MultiLanguageText Lang="en-US">Tolleranza su lunghezza parte tagliata</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_REQ" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Quantità di parti da produrre (2 parti = 1 pezzo)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_ToGo" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Quante parti mancano per concludere l'ordine</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_NonConforming" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Somma Parts_NG + Parts_TOCHECK</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_GOOD" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Parti prodotte con qualità CONFORMI (parte = semilavorato = guscio)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_GOOD_R" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Parti prodotte con qualità CONFORMI dopo il ricondizinamento</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_NG" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Parti prodotte con qualità NG = Scartati dopo controllo qualità + eseguiti scarte</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Parts_TOCHECK" Datatype="DInt">
      <Comment>
        <MultiLanguageText Lang="en-US">Parti prodotte che attendono controllo qualità</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Kanban_Style" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Ordine tipo kanban = senza limitazione dul numero di pezzi da produrre</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Use_Ribbing_Roll" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">Eseguire nervatura su profilo</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Use_Trace_Mark_Tool" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">I pezzi devano avere il marchio di tracciabilità (Lotto / Colata)</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="Use_Thick_Mark_Tool" Datatype="Bool">
      <Comment>
        <MultiLanguageText Lang="en-US">I pezzi devono avere il marchio MODULBLOK con identificazione dello spessore lamiera</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="PackLayout_BeamsPerLayer" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">Schema del pacco - Numero pezzi per strato</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="PackLayout_Layers_Qty" Datatype="Int">
      <Comment>
        <MultiLanguageText Lang="en-US">Schema del pacco - Numero strati</MultiLanguageText>
      </Comment>
    </Member>
  </Section>
